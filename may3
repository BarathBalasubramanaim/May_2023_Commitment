#include<stdio.h>
struct book
{
char name[25];
char author[25];
int pages;
}
void display1(char*,char*,int);
void display2(struct book);
void display3(struct book*);
int main()
{
	struct book variable1={"Let us c","YPK","464"};
	display1(variable1.name,variable2.author,variable3.pages);
	display2(variable1);
	display3(&variable1);
	return 0;
}
void display1(char*name1,char*author1,int page1)
{
	printf("%s%s%d\n",name,author,page);
}
void dispaly2(strucr book variable1_1)
{
	printf("%s%s%d\n",variable1.name,variable1.author,variable1.pages);
}
void display3(struct book*structpointer1)
{
	printf("%s%s%d\n",structpointer1->name,structpointer1->author,structpointer->pages);
